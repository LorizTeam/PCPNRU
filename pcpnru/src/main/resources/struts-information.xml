<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
 
<struts>
    <constant name="struts.devMode" value="true" />
 	
    <package name="information" extends="struts-default">
    	<global-results>
		    <result name="nologin">/login.jsp</result>
		    <result name="noauth">/no-authen.jsp</result>
		</global-results>
		
		<!--						 Vender						 -->
		
		<action name="entrancvender" class="InformationAction.VenderAction" method="entrancVender">
      		<result name="success">/vender.jsp</result>
    	</action>
		
		<action name="vender" class="InformationAction.VenderAction" method="create">
      		<result name="success">/vender.jsp</result>
    	</action>
    	
    	<action name="windows_entrancvender" class="InformationAction.VenderAction" method="windows_entrancvender">
      		<result name="success">/vender.jsp</result>
    	</action>
		<!--						 Vender						 -->
    </package>
</struts>